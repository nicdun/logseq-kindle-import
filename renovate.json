{
  "extends": [
    "config:recommended",
    ":dependencyDashboard"
  ],
  "schedule": [
    "before 2am"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "schedule": [
        "before 2am"
      ],
      "automerge": true,
      "ignoreTests": true
    },
    {
      "matchPackageNames": [
        "@logseq/libs"
      ],
      "ignoreUnstable": false,
      "automerge": false,
      "ignoreTests": true
    }
  ]
}
